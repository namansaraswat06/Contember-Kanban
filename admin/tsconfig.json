{
	"compilerOptions": {
		"isolatedModules": true,
		"jsx": "react",
		"lib": ["esnext", "dom", "dom.iterable"],
		"module": "esnext",
		"moduleResolution": "node",
		"noEmit": true,
		"skipLibCheck": true,
		"strict": true,
		"target": "esnext",
		"types": ["@types/react-bootstrap"],
		"noImplicitAny": false
	}
}
